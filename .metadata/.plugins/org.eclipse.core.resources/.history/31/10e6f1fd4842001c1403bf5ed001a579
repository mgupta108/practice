package java2Blog;

//generate array of n length sum of which is 0

public class OracleTest3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n = 3;
		int sum_temp = 0;
		
		int[] input = new int[n];
		
		for(int i=0; i<n-2; i++) {
			input[i] = i;
			sum_temp += input[i];
		}
	}

}
