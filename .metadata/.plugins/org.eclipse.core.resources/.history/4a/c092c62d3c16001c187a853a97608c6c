import java.util.Iterator;

public class Program7 {

	//find all substring of a string
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		String input = "abcd";
		
		getOrderOne(input);
//		for(int i=0; i<input.length(); i++) {
//			
//		}
		
//		String a = "string";
//		String b = new String("string");
