	public static List<Character> getOrderOne(String in) {
		
		//String[] str= {};
		List<Character> str = new ArrayList<Character>();
		
		
		for(int i =0; i<in.length(); i++) {
			str.add(in.charAt(i));
		}
		
		printList(str);
		return str;
		
	}
	
	public static void printList(List lst) {
		Iterator<Character> itr = lst.iterator();
		while(itr.hasNext()) {
			System.out.print(itr.next());
		}
		System.out.println();
	}
}
