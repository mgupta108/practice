		int len = in.length;
		int sum =0;
		
		for(int i=0; i<len; i++) {
			
			sum =0;
			
			for(int j=0; j<len; j++) {
				
				//if element present at index is equal to the reqdSum, print index range (j,j)
				if(in[j] == reqdSum) {
					System.out.println("Subarray is [" + j + " "+j + " ]");
				}
				
				//getting the sum in range (j , len)
				
