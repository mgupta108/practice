package sorting;

public class InsertionSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] arr = {65,25,12,22,11};
		int index = -1;
		int temp = 0;
		
		for(int i=1; i<arr.length; i++) {
			
			if(arr[i] < arr[i-1]) {
				index = -1;
				for(int j=i; j>=0; j--) {
					if(arr[j]<arr[i]) {
						index = j;
						break;
					}
				}
				
				//shifting elements
				if()
			}
			
			
			
		}
	}

}
